{
    "background": {
       "page": "background.htm"
    },
    "browser_action": {
       "default_icon": "check.png",
       "default_popup": "popup.html",
       "default_title": "Page checker"
    },
    "description": "Check for pages changes",
    "manifest_version": 2,
    "name": "Page checker",
    "short_name":"Page checker",
    "permissions": [
        "storage",
        "tabs", 
        "http://*/", 
        "https://*/" 
    ],   
    "version": "2.0.0"
}
